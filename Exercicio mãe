//13 Crie um programa que imprima a tabuada de multiplicação de um número fornecido pelo usuário.//
#include <stdio.h>

int main() {
    int tabuada;
    int n;
    int i = 1;
    
    printf("Escolha um número: ");
    scanf("%d", &n);
    while(i <= 10){
        tabuada = n * i;
        i++;
        printf("%d\n" , tabuada);
    }
    return 0;
}

/*Faça um programa que conte de 1 a 100 e imprima "Fizz" para números divisíveis por 3 e "Buzz" para números divisíveis por 5. Para números que são divisíveis por ambos, imprima "FizzBuzz".*/

#include <stdio.h>
int main(){
    
    int n;
    int i = 1;
    
    while(i <= 100){
        
        n = n + i;
        i++;
        
        if(i % 3 == 0){
            printf("Fizz");
        }
        else if(i % 5 == 0){
            printf("Buzz");
        }
        else{
            printf("FIZBUZZ");
        }
    }
    
return 0;
}
